        "swoole",
        "swoft",
        "swoft-component",
        "http-session",
        "session"
    ],
    "description": "Http session component for swoft",
    "license": "Apache-2.0",
    "require": {
        "swoft/http-message": "~2.0.0",
        "swoft/stdlib": "~2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
